    "walk-sync": "^0.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.1",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "dotenv": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
