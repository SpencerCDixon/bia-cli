{
  "name": "bia-cli",
  "version": "0.0.1",
  "description": "Personal productivity cli",
  "main": "bin/index.js",
  "engine": {
    "node": ">=5.1"
  },
  "scripts": {
    "start": "npm run build:watch",
    "build": "babel src -d lib",
    "build:watch": "babel src --watch -d lib",
    "lint": "eslint ./src ./test",
    "clean": "rimraf lib"
  },
  "keywords": [
    "cli",
    "habits",
    "productivity"
  ],
  "author": "Spencer Dixon <spencercdixon@gmail.com>",
  "license": "MIT",
  "bin": {
    "bia": "bin/index.js",
    "bia-goals": "bin/bia-goals.js",
    "bia-init": "bin/bia-init.js",
    "bia-habit": "bin/bia-habit.js",
    "bia-weight": "bin/bia-weight.js"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "denodeify": "^1.2.1",
    "ejs": "^2.4.1",
    "elegant-spinner": "^1.0.1",
    "figlet": "^1.1.1",
    "fs-extra": "^0.26.5",
    "humps": "^1.0.0",
    "jsonfile": "^2.2.3",
    "lodash": "^4.5.1",
    "log-update": "^1.0.2",
    "minimist": "^1.2.0",
    "moment": "^2.14.1",
    "prompt": "^1.0.0",
